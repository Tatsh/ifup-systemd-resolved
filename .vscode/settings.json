{
  "[c]": {
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.tabSize": 4
  },
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "addWords": true,
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:rc-scripts}/.vscode/custom-dictionary-workspace.txt",
      "scope": "workspace"
    }
  },
  "cmake.configureArgs": ["-DCMAKE_INSTALL_PREFIX=/usr"],
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.associations": {
    "**/bash-completion/completions/*": "shellscript",
    "*.json.dist": "json",
    ".bash_functions": "shellscript",
